- content_for :title, t('.title')

- content_for :action_bar, link_to(t('.cancel'), users_path, :class => 'button')

= form_for @config, url: site_config_index_path, method: 'put' do |f|
  = errors_for @config

  %fieldset
    %legend= t('notice_fingerprinter.title')
    %p= t('notice_fingerprinter.text')

    = render 'shared/notice_fingerprinter', :f => f

  %div.buttons= f.submit t('.update_config')
