/* Issue Tracker inactive, select, create and goto icons */
<% trackers = IssueTracker.subclasses.map{|t| t.label } << 'none' %>

<% trackers.each do |tracker| %>
div.issue_tracker.nested label.<%= tracker %> {
  background: url(/assets/<%= tracker %>_inactive.png) no-repeat;
}
div.issue_tracker.nested label.r_on.<%= tracker %> {
  background: url(/assets/<%= tracker %>_create.png) no-repeat;
}
#action-bar a.<%= tracker %>_create {
   background: transparent url(/assets/<%= tracker %>_create.png) 6px 5px no-repeat;
}
#action-bar a.<%= tracker %>_goto {
  background: transparent url(/assets/<%= tracker %>_goto.png) 6px 5px no-repeat;
}
<% end %>

